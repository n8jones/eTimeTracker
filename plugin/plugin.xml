<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.ui.editors">
		<editor class="me.natejones.ett.Editor" default="true" extensions="timelog"
			id="me.natejones.ett.Editor" name="TimeLog Editor">
		</editor>
	</extension>
   	<extension point = "org.eclipse.ui.newWizards">
		<wizard id = "me.natejones.ett.timelog.newfile" name = "Time Log" class="me.natejones.ett.NewFileWizard">
			<description>Create a new time log file.</description>
			<selection class="org.eclipse.core.resources.IResource"/>
		</wizard>
	</extension>
</plugin>
